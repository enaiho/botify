<!doctype html>
<html lang="en">
<head>
    
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="../assets/img/apple-icon.png" />
    <link rel="icon" type="image/png" href="../assets/img/favicon.png" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>BOTIfy | Queries</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <!-- Bootstrap core CSS     -->
    <link href="/css/bootstrap.min.css" rel="stylesheet" />
    <!--  Material Dashboard CSS    -->
    <link href="/css/material-dashboard.css?v=1.2.0" rel="stylesheet" />
    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="/css/demo.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons' rel='stylesheet' type='text/css'>


</head>

<body>
    
    
    <div class="wrapper">
        
        
        
        <%- sidebar %>
        
        
        
        <div class="main-panel">
            <nav class="navbar navbar-transparent navbar-absolute">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#"> Queries/Chats </a>
                    </div>
                    <div class="collapse navbar-collapse">
                        
                        <%- header %>
                        
                    </div>
                </div>
            </nav>
            <div class="content">
                <div class="container-fluid">
				
				<div class="row">
				    
				       
				        
				        
                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-header" data-background-color="purple">
                                    <h4 class="title">Queries <strong>(<%= queries.length %>)</strong> </h4>
                                    
                                </div>
                                
                                
                                
                                
                                            
                                <% if(queries.length == 0){ %>
                                
                                    
                                    <br />
                                    <br />
                                    
                                    <div align="center">
                                        <strong><h6>No queries/chats have been sent to any of your bots</h6></strong>
                                    </div>
                                    
                                    <br />
                                    <br />
                                
                                <% }
                                
                                else{
                                    
                                    
                                    %>
                                    
                                    <div class="card-content table-responsive">
                                    
                                        <table class="table">
                                            
                                            <thead class="text-primary">
                                                
                                                
                                                <tr>
                                                    
                                                    <td>Question/Chat</td>
                                                    <td>Value/Response</td>
                                                    <td>Bot</td>
                                                    <td>Responded</td>
                                                    <td></td>
                                                
                                                
                                                </tr>
                                                
                                                <br />
                                                
                                                
    
                                            </thead>
                                            <tbody>
                                                
                                    
                                    
                                    <%
                                
                                    for(var i=0; i<queries.length; i++){
                                        
                                %>
                                
                                    
                                    <tr>
                                        
                                        
                                        <td> <%= queries[i].query %> </td>
                                        <td> <strong><%= queries[i].val %></strong> </td>
                                        <td> 
                                        
                                        <%= queries[i].bot_name %>
                                       
                                        </td>
                                        <td> 
                                        
                                        
                                        <% 
                                        
                                        var responded_val = "";
                                        if(queries[i].response == 0)
                                            responded_val = "No";
                                        else
                                            responded_val = "Yes";
                                        
                                        
                                        %>
                                        
                                        
                                        <b><%= responded_val %></b> 
                                        
                                        
                                        </td>
                                        
                                        <%
                                        
                                        if(queries[i].response == 0){
                                        %>
                                            <td><a href="javascript:void();" class="cls_learn_add" id="learn_<%= queries[i].bot_id %>_<%= queries[i].query %>_<%= queries[i]._id %> ">Add Question to Bot</a></td>
                                        
                                        <%
                                        }
                                        else if(queries[i].response == 2)
                                        {
                                        %>
                                            <td>>Knowledgebase updated</td>
                                        <%
                                        }
                                        
                                        %>
                                        
                                    </tr>
                                
                                    
                                        
                                <%
                                        
                                    }
                                    
                                    
                                    
                                    %>
                                    
                                        </tbody>
                                    
                                    </table>
                                    
                                </div>
                                    
                                    <%
                                    
                                }
                                
                                
                                
                                %>
                                
                                
                            </div>
                            
                        </div>
                        
                        

</div>


                        </div>


</div>


<div id="AddQuestion" class="modal fade" role="dialog">
    
    
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Add <span id="lbl_ques"></span> to Bot</h4>
      </div>
      
      <div class="modal-body">
        
        <form method="post">
            
            <div class="row">
                
                
                <div class="col-md-12">
                    
                    <div class="form-group">
                        <div class="form-group label-floating">
                            
                            <label class="control-label">Select Category</label><br /><br />
                            <select class="form-control" id="append_cats">
                            </select>
                            
                            
                        </div>
                        
                    </div>
                    
                </div>
                
                <div class="col-md-12">
                    <div class="form-group">
                        <div class="form-group label-floating">
                            
                            <label class="control-label">Enter the response you want your users to get</label><br />
                            <textarea class="form-control" rows="3" id="value"></textarea>
                            <input type="hidden" id="question" />
                            
                            
                        </div>
                        
                    </div>
                    
                </div>
                
                
            </div>
            
            <br />
            
            <button type="submit" class="btn btn-danger pull-right" data-dismiss="modal">Close</button>
            <button type="submit" id="btn_learn" class="btn btn-primary pull-right">Save</button>
            <div class="clearfix"></div>
            
            
            
        </form>
      
      
      </div>
      
      
      <div class="modal-footer">
      </div>
    
    
    </div>
    
    <input type="hidden" id="bot_id" />
    <input type="hidden" id="query_id" />
    

  </div>
  
</div>



</body>



<!--   Core JS Files   -->
<script src="/js/jquery-3.2.1.min.js" type="text/javascript"></script>
<script src="/js/bootstrap.min.js" type="text/javascript"></script>
<script src="/js/material.min.js" type="text/javascript"></script>
<script src="/js/chartist.min.js"></script>
<script src="/js/arrive.min.js"></script>
<script src="/js/perfect-scrollbar.jquery.min.js"></script>
<script src="/js/bootstrap-notify.js"></script>
<script src="/js/material-dashboard.js?v=1.2.0"></script>
<script src="/js/demo.js"></script>
<script src="js/app.js"></script>
<script type="text/javascript">
    
    
    $(document).ready(function() {

        // Javascript method's body can be found in assets/js/demos.js
        demo.initDashboardPageCharts();

    });
    
    
</script>
</html>



